{
    "name": "spryker-sdk/spryk-gui",
    "type": "library",
    "description": "SprykGui module",
    "license": "proprietary",
    "require": {
        "php": ">=7.3",
        "ondrejmirtes/better-reflection": "^4.3.0",
        "spryker-sdk/spryk": "dev-bugfix/te-9771-php8 as 0.3.1",
        "spryker/development": "^3.12.0",
        "spryker/graph": "^3.0.0",
        "spryker/kernel": "^3.30.0",
        "spryker/laminas": "^1.0.0",
        "spryker/symfony": "^3.5.0"
    },
    "require-dev": {
        "codeception/module-asserts": "^1.3",
        "codeception/module-phpbrowser": "^1.0",
        "phpstan/phpstan": "^0.12.0",
        "spryker/application": "*",
        "spryker/code-sniffer": "*",
        "spryker/container": "*",
        "spryker/event-dispatcher": "*",
        "spryker/form": "*",
        "spryker/gui": "*",
        "spryker/propel": "*",
        "spryker/router": "*",
        "spryker/testify": "*",
        "spryker/twig": "*",
        "spryker/validator": "*",
        "spryker/zed-navigation": "*"
    },
    "autoload": {
        "psr-4": {
            "SprykerSdk\\": "src/SprykerSdk/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerSdkTest\\": "tests/SprykerSdkTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml --ignore=/tests/app/ src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml --ignore=/tests/app/ src/ tests/",
        "stan": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/codecept run"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
