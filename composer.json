{
    "name": "spryker-sdk/spryk-gui",
    "type": "library",
    "description": "SprykGui module",
    "license": "proprietary",
    "require": {
        "php": ">=7.3",
        "roave/better-reflection": "^4.3.0",
        "spryker-sdk/spryk": "^0.3.0",
        "spryker/development": "^3.12.0",
        "spryker/graph": "^3.0.0",
        "spryker/kernel": "^3.30.0",
        "spryker/laminas": "^1.0.0",
        "spryker/symfony": "^3.5.0"
    },
    "require-dev": {
        "spryker/application": "*",
        "spryker/code-sniffer": "*",
        "spryker/container": "*",
        "spryker/event-dispatcher": "*",
        "spryker/form": "*",
        "spryker/gui": "*",
        "spryker/propel": "*",
        "spryker/router": "*",
        "spryker/testify": "*",
        "spryker/twig": "*",
        "spryker/validator": "*",
        "spryker/zed-navigation": "*"
    },
    "autoload": {
        "psr-4": {
            "SprykerSdk\\": "src/SprykerSdk/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerSdkTest\\": "tests/SprykerSdkTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
