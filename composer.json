{
  "name": "spryker-sdk/spryk-gui",
  "type": "library",
  "description": "SprykGui module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker-sdk/spryk": "^0.2.2",
    "spryker/development": "^3.12.0",
    "spryker/graph": "^3.0.0",
    "spryker/kernel": "^3.30.0",
    "spryker/symfony": "^3.0.0",
    "spryker/zend": "^2.0.0"
  },
  "require-dev": {
    "spryker/application": "*",
    "spryker/code-sniffer": "*",
    "spryker/propel": "*",
    "spryker/testify": "*",
    "spryker/zed-navigation": "*"
  },
  "autoload": {
    "psr-4": {
      "SprykerSdk\\": "src/SprykerSdk/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerSdkTest\\": "tests/SprykerSdkTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml -p --ignore=/Fixtures/ src/ tests/",
    "cs-fix": "phpcbf --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml -p --ignore=/Fixtures/ src/ tests/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
